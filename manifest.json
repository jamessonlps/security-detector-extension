{
  "browser_action": {
    "browser_style": true,
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/biscoitos16.png",
      "32": "icons/biscoitos32.png",
      "48": "icons/biscoitos32.png"
    }
  },
  "manifest_version": 2,
  "name": "James Security",
  "version": "1.0",
  "permissions": [
    "cookies",
    "<all_urls>",
    "tabs",
    "storage",
    "browsingData",
    "notifications",
    "activeTab",
    "contextMenus",
    "webRequest"
  ],
  "background": {
    "scripts": [
      "js/utils.js",
      "jquery-3.6.0.min.js",
      "index.js",
      "js/getRequests.js"
    ]
  },
  "content_security_policy": "script-src 'self' https://*.jquery.com; object-src 'self'"
}